<Window x:Class="InternshipTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:local="clr-namespace:InternshipTest"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:sfSkinManager="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
		xmlns:vehicle="clr-namespace:InternshipTest.Vehicle"
		xmlns:simulation="clr-namespace:InternshipTest.Simulation"
		xmlns:results="clr-namespace:InternshipTest.Results"
		xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
		Title="MainWindow" Height="1080" Width="1920" WindowState="Maximized" Icon="App.ico">
    <Window.Resources>
        <ItemsPanelTemplate x:Key="chartsLegendsPanelTemplate">
            <WrapPanel Orientation="Horizontal"/>
        </ItemsPanelTemplate>
    </Window.Resources>
    <!-- Main Window Grid -->
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <!-- Main Navigation Grid -->
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <!-- Main Options Grid -->
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                </Grid.ColumnDefinitions>
                <Button Name="projectEnvironmentButton" Content="Project" Grid.Column="0"
                        Height="30" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold" FontSize="15"
                        Click="_ProjectEnvironmentButton_Click"/>
                <Button Name="vehicleInputEnvironmentButton" Content="Vehicle" Grid.Column="1"
                        Height="30" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold" FontSize="15"
                        Click="_VehicleInputEnvironmentButton_Click"/>
                <Button Name="pathInputEnvironmentButton" Content="Path" Grid.Column="2"
                        Height="30" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold" FontSize="15"
                        Click="_PathInputEnvironmentButton_Click"/>
                <Button Name="simulationInputEnvironmentButton" Content="Simulation" Grid.Column="3" 
                        Height="30" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold" FontSize="15"
                        Click="_SimulationInputEnvironmentButton_Click"/>
                <Button Name="resultsAnalysisEnvironmentButton" Content="Results" Grid.Column="4"
                        Height="30" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold" FontSize="15"
                        Click="_ResultsAnalysisEnvironmentButton_Click"/>
                <Button Name="helpEnvironmentButton" Content="Help" Grid.Column="5" HorizontalAlignment="Right"
                        Height="30" Margin="0,10,20,10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold" FontSize="15"
                        Click="_HelpEnvironmentButton_Click"/>
                <TextBlock Text="Theme:" Grid.Column="6" VerticalAlignment="Center" 
                           FontSize="15" FontWeight="Bold"/>
                <ComboBox Name="themeSelectionCombobox" Grid.Column="7"
                          Height="30" Width="150" Margin="10" HorizontalAlignment="Right"
                          VerticalContentAlignment="Center"
                          SelectionChanged="_ThemeSelectionCombobox_SelectionChanged">
                    <ComboBoxItem Content="Blend"/>
                    <ComboBoxItem Content="Metro"/>
                    <ComboBoxItem Content="SystemTheme"/>
                    <ComboBoxItem Content="Office2010Black"/>
                    <ComboBoxItem Content="Office2010Blue"/>
                    <ComboBoxItem Content="Office2010Silver"/>
                    <ComboBoxItem Content="Office2013White"/>
                    <ComboBoxItem Content="Office2013LightGray"/>
                    <ComboBoxItem Content="Office2013DarkGray"/>
                    <ComboBoxItem Content="Office2016Colorful"/>
                    <ComboBoxItem Content="Office2016White"/>
                    <ComboBoxItem Content="Office2016DarkGray"/>
                    <ComboBoxItem Content="Office365"/>
                    <ComboBoxItem Content="VisualStudio2013"/>
                    <ComboBoxItem Content="VisualStudio2015"/>
                </ComboBox>
            </Grid>
            <!-- Project Button Options Grid -->
            <Grid Name="projectButtonOptionsGrid" Grid.Row="1" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <!-- Question textbox -->
                <TextBlock Text="What would you like to do?" VerticalAlignment="Center" Margin="10"/>
                <!-- New project button -->
                <Button Name="newProjectButton" Content="New" Grid.Column="1"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_NewProjectButton_Click"/>
                <!-- Save project button -->
                <Button Name="saveProjectButton" Content="Save" Grid.Column="2"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_SaveProjectButton_Click"/>
                <!-- Load project button -->
                <Button Name="loadProjectButton" Content="Load" Grid.Column="3"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_LoadProjectButton_Click"/>
            </Grid>
            <!-- Vehicle Button Options Grid -->
            <Grid Name="vehicleButtonOptionsGrid" Grid.Row="1" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <!-- Question textbox -->
                <TextBlock Text="Choose what you want to model:" VerticalAlignment="Center" Margin="10"/>
                <!-- Tire button -->
                <Button Name="tireButton" Content="Tire" Grid.Column="1"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_TireButton_Click"/>
                <!-- One Wheel Model button -->
                <Button Name="oneWheelVehicleButton" Content="One Wheel" Grid.Column="2"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_OneWheelVehicleButton_Click"/>
                <!-- Two Wheel Model button -->
                <Button Name="twoWheelVehicleButton" Content="Two Wheel" Grid.Column="3"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_TwoWheelVehicleButton_Click"/>
                <!-- Four Wheel Model button -->
                <Button Name="fourWheelVehicleButton" Content="Four Wheel" Grid.Column="4"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_FourWheelVehicleButton_Click"/>
            </Grid>
            <!-- Path Button Options Grid -->
            <Grid Name="pathButtonOptionsGrid" Grid.Row="1" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <!-- Question textbox -->
                <TextBlock Text="Choose an input method:" VerticalAlignment="Center" Margin="10"/>
                <!-- Tabular Path button -->
                <Button Name="tabularPathButton" Content="Tabular" Grid.Column="1"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_TabularPathButton_Click"/>
                <!-- Draw Path button -->
                <Button Name="drawPathButton" Content="Draw" Grid.Column="2"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_DrawPathButton_Click"/>
                <!-- Optimize Path button -->
                <Button Name="optimizePathButton" Content="Optimization" Grid.Column="3"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_OptimizePathButton_Click"/>
            </Grid>
            <!-- Simulation Button Options Grid -->
            <Grid Name="simulationButtonOptionsGrid" Grid.Row="1" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <!-- Question textbox -->
                <TextBlock Text="Choose a simulation type:" VerticalAlignment="Center" Margin="10"/>
                <!-- GGV Diagram button -->
                <Button Name="ggvDiagramButton" Content="GGV Diagram" Grid.Column="1"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_GGVDiagramButton_Click"/>
                <!-- Lap Time Simulation button -->
                <Button Name="lapTimeSimulationButton" Content="Lap Time" Grid.Column="2"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_LapTimeSimulationButton_Click"/>
            </Grid>
            <!-- Results Button Options Grid -->
            <Grid Name="resultsButtonOptionsGrid" Grid.Row="1" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <!-- Question textbox -->
                <TextBlock Text="Choose a simulation type:" VerticalAlignment="Center" Margin="10"/>
                <!-- GGV Diagram button -->
                <Button Name="ggvDiagramResultsAnalysisButton" Content="GGV Diagram" Grid.Column="1"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_GGVDiagramResultsAnalysisButton_Click"/>
                <!-- Lap Time Simulation button -->
                <Button Name="lapTimeSimulationResultsAnalysisButton" Content="Lap Time" Grid.Column="2"
                        Height="25" Margin="10" Background="Transparent" BorderBrush="Transparent"
                        FontWeight="Bold"
                        Click="_LapTimeSimulationResultsAnalysisButton_Click"/>
            </Grid>
        </Grid>
        <!-- Main Work Environment Docking Manager -->
        <syncfusion:DockingManager Name="mainWorkEnvironmentDockingManager" Grid.Row="2"
                                   MaximizeButtonEnabled="True" DockFill="True">
            <ContentControl Name="projectOverview" MinWidth="50"
                            syncfusion:DockingManager.IsContextMenuButtonVisible="False"
                            syncfusion:DockingManager.CanClose="False"
                            syncfusion:DockingManager.CanFloat="False"
                            syncfusion:DockingManager.Header="Overview"
                            syncfusion:DockingManager.SidePanelDock="Left"
                            syncfusion:DockingManager.SideInDockedMode="Left"
                            syncfusion:DockingManager.DesiredWidthInDockedMode="250"/>
            <ContentControl Name="taskTracker" MinWidth="50"
                            syncfusion:DockingManager.IsContextMenuButtonVisible="False"
                            syncfusion:DockingManager.CanClose="False"
                            syncfusion:DockingManager.CanFloat="False"
                            syncfusion:DockingManager.Header="Task Tracker"
                            syncfusion:DockingManager.SidePanelDock="Right"
                            syncfusion:DockingManager.SideInDockedMode="Right"
                            syncfusion:DockingManager.DesiredWidthInDockedMode="250"/>
            <ContentControl Name="commentsBox" MinHeight="10"
                            syncfusion:DockingManager.IsContextMenuButtonVisible="False"
                            syncfusion:DockingManager.CanClose="False"
                            syncfusion:DockingManager.CanFloat="False"
                            syncfusion:DockingManager.Header="Comments"
                            syncfusion:DockingManager.SidePanelDock="Bottom"
                            syncfusion:DockingManager.SideInDockedMode="Bottom"
                            syncfusion:DockingManager.DesiredHeightInDockedMode="10">
                <TextBox Name="commentsTextBox" Text="Insert project's comments here." TextWrapping="WrapWithOverflow"/>
            </ContentControl>
            <ContentControl Name="mainWorkEnvironment" MinHeight="10"
                            syncfusion:DockingManager.IsContextMenuButtonVisible="False"
                            syncfusion:DockingManager.CanClose="False"
                            syncfusion:DockingManager.CanFloat="False"
                            syncfusion:DockingManager.Header="Main"
                            syncfusion:DockingManager.SidePanelDock="Top"
                            syncfusion:DockingManager.SideInDockedMode="Top">
                <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                    <!-- Main Work Environment Grid -->
                    <Grid Name="mainWorkGrid">
                        <Grid.Resources>
                            <!-- Template of the positioning of the charts legends so they wrap -->
                            <ItemsPanelTemplate x:Key="chartsLegendsPanelTemplate">
                                <WrapPanel/>
                            </ItemsPanelTemplate>
                        </Grid.Resources>
                        <!-- Tire Docking Manager -->
                        <syncfusion:DockingManager Name="tireInputDockingManager" Visibility="Collapsed">
                            <!-- Tire Docking Window -->
                            <ContentControl Name="tireDockingWindow"
                                        syncfusion:DockingManager.IsContextMenuButtonVisible="False"
                                        syncfusion:DockingManager.CanClose="False"
                                        syncfusion:DockingManager.CanFloat="False"
                                        syncfusion:DockingManager.Header="Tire"
                                        syncfusion:DockingManager.DesiredWidthInDockedMode="250"
                                        syncfusion:DockingManager.DesiredMinWidthInDockedMode="250"
                                        syncfusion:DockingManager.State="AutoHidden">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="100"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <!-- Tire listbox-->
                                    <ListBox Name="tireListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="6" Margin="5" SelectionChanged="_TireListBox_SelectionChanged"/>
                                    <!-- Tire listbox add and delete buttons -->
                                    <Button Name="addTireToListBox" Content="Add" Click="_AddTireToListBox_Click" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Margin="5,2,5,2"/>
                                    <Button Name="deleteTireOfListBox" Content="Delete" Click="_DeleteTireOfListBox_Click" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="3" Margin="5,2,5,2"/>
                                    <!-- Tire ID line -->
                                    <TextBlock Text="Tire ID" Grid.Row="2" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="5,0,0,0"/>
                                    <TextBox Name="tireIDTextBox" Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="4" Margin="5,2,5,2"/>
                                    <!-- Description line -->
                                    <TextBlock Text="Description" Grid.Row="3" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="5,2,5,2"/>
                                    <TextBox Name="tireDescriptionTextBox" 
                                         Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="4" Margin="5,2,5,2" TextWrapping="WrapWithOverflow"/>
                                    <!-- Tire vertical stiffness line -->
                                    <TextBlock Text="Vert. Stiffness" Grid.Row="4" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="5,0,0,0"/>
                                    <syncfusion:DoubleTextBox Name="tireStiffnessTextBox" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="3"
                                                          Margin="5,0,5,0" HorizontalContentAlignment="Right" NumberDecimalDigits="3"/>
                                    <TextBlock Text="N/mm" Grid.Row="4" Grid.Column="5" VerticalAlignment="Center"/>
                                    <!-- Tire model file selection line -->
                                    <TextBlock Text="Tire Model" Grid.Row="5" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="5,0,0,0"/>
                                    <ComboBox Name="tireModelComboBox" Grid.Row="5" Grid.Column="2" Grid.ColumnSpan="4" Margin="5,2,5,2"
                                              ItemsSource="{Binding ElementName=tireModelListBox, Path=Items, Mode=OneWay}"/>
                                </Grid>
                            </ContentControl>
                            <!-- Tire Model Docking Window -->
                            <ContentControl Name="tireModelDockingWindow"
                                        syncfusion:DockingManager.IsContextMenuButtonVisible="False"
                                        syncfusion:DockingManager.CanClose="False"
                                        syncfusion:DockingManager.CanFloat="False"
                                        syncfusion:DockingManager.Header="Tire Model"
                                        syncfusion:DockingManager.DesiredWidthInDockedMode="250"
                                        syncfusion:DockingManager.SideInDockedMode="Left"
                                        syncfusion:DockingManager.DesiredMinWidthInDockedMode="250"
                                        syncfusion:DockingManager.State="AutoHidden">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="100"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <!-- Tire Model listbox-->
                                    <ListBox Name="tireModelListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="6" Margin="5" SelectionChanged="_TireModelListBox_SelectionChanged"/>
                                    <!-- Tire Model listbox add and delete buttons -->
                                    <Button Name="oneWheelAddTireModelToListBox" Content="Add" Click="_AddTireModelToListBox_Click" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Margin="5,2,5,2"/>
                                    <Button Name="oneWheelDeleteTireModelOfListBox" Content="Delete" Click="_DeleteTireModelOfListBox_Click" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="3" Margin="5,2,5,2"/>
                                    <!-- Tire Model ID line -->
                                    <TextBlock Text="Model ID" Grid.Row="2" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="5"/>
                                    <TextBox Name="tireModelIDTextBox" Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="4" Margin="5,2,5,2"/>
                                    <!-- Description line -->
                                    <TextBlock Text="Description" Grid.Row="3" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="5,2,5,2"/>
                                    <TextBox Name="tireModelDescriptionTextBox" 
                                         Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="4" Margin="5,2,5,2" TextWrapping="WrapWithOverflow"/>
                                    <!-- Tire model file selection line -->
                                    <TextBlock Text="Tire Model File Selection:" Grid.Row="4" Grid.ColumnSpan="6" VerticalAlignment="Center" Margin="5,2,5,2"/>
                                    <TextBox Name="tireModelTextBox" Grid.Row="5" Grid.Column="2" Grid.ColumnSpan="4" Margin="5,2,5,2"/>
                                    <Button Name="oneWheelTireModelButton" Content="Browse" Click="_TireModelButton_Click" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="2" Margin="5,2,5,2"/>
                                    <!-- Scalling Factors textblock -->
                                    <TextBlock Text="Scalling Factors:" Grid.Row="6" Grid.ColumnSpan="6" VerticalAlignment="Center" Margin="5,2,5,2"/>
                                    <!-- lambdaFzO -->
                                    <TextBlock HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center"
                                           Grid.Row="7" Grid.Column="0" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="Fzo"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaFzOTextBox" Text="1" Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="1" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaMuV -->
                                    <TextBlock Grid.Row="7" Grid.Column="3" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="μV"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaMuVTextBox" Text="0" Grid.Row="7" Grid.Column="4" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="2" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaMux -->
                                    <TextBlock Grid.Row="8" Grid.Column="0" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="μx"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaMuxTextBox" Text=".66" Grid.Row="8" Grid.Column="1" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="2" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaMuy -->
                                    <TextBlock Grid.Row="8" Grid.Column="3" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="μy"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaMuyTextBox" Text=".66" Grid.Row="8" Grid.Column="4" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="2" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaKxk -->
                                    <TextBlock Grid.Row="9" Grid.Column="0" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="xκ"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaKxkTextBox" Text="1" Grid.Row="9" Grid.Column="1" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="2" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaKya -->
                                    <TextBlock Grid.Row="9" Grid.Column="3" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="yα"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaKyaTextBox" Text="1" Grid.Row="9" Grid.Column="4" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="2" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaCx -->
                                    <TextBlock Grid.Row="10" Grid.Column="0" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="10" Text="Cx"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaCxTextBox" Text="1" Grid.Row="10" Grid.Column="1" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="2" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaCy -->
                                    <TextBlock Grid.Row="10" Grid.Column="3" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="Cy"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaCyTextBox" Text="1" Grid.Row="10" Grid.Column="4" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="2" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaEx -->
                                    <TextBlock Grid.Row="11" Grid.Column="0" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="Ex"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                    <syncfusion:DoubleTextBox Name="lambdaExTextBox" Text="1" Grid.Row="11" Grid.Column="1" Grid.ColumnSpan="2"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" 
														  Margin="2" NumberDecimalDigits="3"></syncfusion:DoubleTextBox>
                                    <!-- lambdaEy -->
                                    <TextBlock Grid.Row="11" Grid.Column="3" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" 
										   FontSize="15" >
									<Run>λ</Run>
