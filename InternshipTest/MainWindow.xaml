<syncfusion:RibbonWindow x:Class="InternshipTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:local="clr-namespace:InternshipTest"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:sfSkinManager="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
		xmlns:oneWheel="clr-namespace:InternshipTest.Vehicle.OneWheel"
		xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
		Title="MainWindow" Height="1080" Width="1920" WindowState="Maximized" Icon="App.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <syncfusion:Ribbon x:Name="mainRibbon" Grid.Row="0" VerticalAlignment="Top" SelectionChanged="MainRibbon_SelectionChanged" >
            <!-- Backstage Environment -->
            <syncfusion:Ribbon.BackStage>
                <syncfusion:Backstage>
                    <syncfusion:BackStageCommandButton Header="New"/>
                    <syncfusion:BackStageCommandButton Header="Save"/>
                    <syncfusion:BackStageCommandButton Header="Save As"/>
                    <syncfusion:BackStageCommandButton Header="Load"/>
                    <syncfusion:BackstageTabItem Header="Options"/>
                    <syncfusion:BackstageTabItem Header="Help"/>
                </syncfusion:Backstage>
            </syncfusion:Ribbon.BackStage>
            <!-- Ribbon Environment -->
            <syncfusion:RibbonTab Name="vehicleInputTab" Caption="Vehicle" IsChecked="False"/>
            <syncfusion:RibbonTab Name="pathInputTab" Caption="Path"  IsChecked="False"/>
            <syncfusion:RibbonTab Name="simulationInputTab" Caption="Simulation"  IsChecked="False"/>
            <syncfusion:RibbonTab Name="resultsTab" Caption="Results"  IsChecked="False"/>
        </syncfusion:Ribbon>
        <!-- Main Grid -->
        <Grid Height="auto" Width="auto" Grid.Row="1">
            <Grid.RowDefinitions>
                <RowDefinition MinHeight="150"/>
                <RowDefinition MinHeight="50" Height="150"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition MinWidth="150" Width="250"/>
                <ColumnDefinition MinWidth="150"/>
                <ColumnDefinition MinWidth="150" Width="250"/>
            </Grid.ColumnDefinitions>
            <!-- Tree view: global view of the project -->
            <syncfusion:TreeViewAdv Grid.Column="0" Grid.Row="0" Grid.RowSpan="2">
                <!-- Vehicles folder-->
                <syncfusion:TreeViewItemAdv Header="Vehicles">
                    <!-- One wheel models folder -->
                    <syncfusion:TreeViewItemAdv Header="One Wheel Model">
                        <!-- Cars list -->
                        <syncfusion:TreeViewItemAdv Header="Cars"/>
                        <!-- Components subfolder -->
                        <syncfusion:TreeViewItemAdv Header="Components">
                            <!-- Components lists -->
                            <syncfusion:TreeViewItemAdv Header="Inertia"/>
                            <syncfusion:TreeViewItemAdv Header="Tires"/>
                            <syncfusion:TreeViewItemAdv Header="Engine"/>
                            <syncfusion:TreeViewItemAdv Header="Transmission"/>
                            <syncfusion:TreeViewItemAdv Header="Aerodynamics"/>
                            <syncfusion:TreeViewItemAdv Header="Suspension"/>
                            <syncfusion:TreeViewItemAdv Header="Brakes"/>
                        </syncfusion:TreeViewItemAdv>
                    </syncfusion:TreeViewItemAdv>
                    <!-- Two wheels models folder -->
                    <syncfusion:TreeViewItemAdv Header="Two Wheels Model">
                        <!-- Cars list -->
                        <syncfusion:TreeViewItemAdv Header="Cars"/>
                        <!-- Components subfolder -->
                        <syncfusion:TreeViewItemAdv Header="Components">
                            <!-- Components lists -->
                            <syncfusion:TreeViewItemAdv Header="Inertia"/>
                            <syncfusion:TreeViewItemAdv Header="Tires"/>
                            <syncfusion:TreeViewItemAdv Header="Engine"/>
                            <syncfusion:TreeViewItemAdv Header="Transmission"/>
                            <syncfusion:TreeViewItemAdv Header="Aerodynamics"/>
                            <syncfusion:TreeViewItemAdv Header="Suspension"/>
                            <syncfusion:TreeViewItemAdv Header="Brakes"/>
                        </syncfusion:TreeViewItemAdv>
                    </syncfusion:TreeViewItemAdv>
                    <!-- Four wheels models folder -->
                    <syncfusion:TreeViewItemAdv Header="Four Wheels Model">
                        <!-- Cars list -->
                        <syncfusion:TreeViewItemAdv Header="Cars"/>
                        <!-- Components subfolder -->
                        <syncfusion:TreeViewItemAdv Header="Components">
                            <!-- Components lists -->
                            <syncfusion:TreeViewItemAdv Header="Inertia"/>
                            <syncfusion:TreeViewItemAdv Header="Tires"/>
                            <syncfusion:TreeViewItemAdv Header="Engine"/>
                            <syncfusion:TreeViewItemAdv Header="Transmission"/>
                            <syncfusion:TreeViewItemAdv Header="Aerodynamics"/>
                            <syncfusion:TreeViewItemAdv Header="Suspension"/>
                            <syncfusion:TreeViewItemAdv Header="Brakes"/>
                        </syncfusion:TreeViewItemAdv>
                    </syncfusion:TreeViewItemAdv>
                </syncfusion:TreeViewItemAdv>
                <!-- Paths folder -->
                <syncfusion:TreeViewItemAdv Header="Paths">
                    <!-- Tabular input paths list -->
                    <syncfusion:TreeViewItemAdv Header="Tabular"/>
                    <!-- Drawed input paths list -->
                    <syncfusion:TreeViewItemAdv Header="Drawing"/>
                    <!-- Optimized paths list -->
                    <syncfusion:TreeViewItemAdv Header="Optimization"/>
                </syncfusion:TreeViewItemAdv>
                <!-- Simulations folder -->
                <syncfusion:TreeViewItemAdv Header="Simulations">
                    <!-- Simulations types subfolders -->
                    <syncfusion:TreeViewItemAdv Header="GGV Diagram"/>
                    <syncfusion:TreeViewItemAdv Header="Lap Time Simulation - Single"/>
                    <syncfusion:TreeViewItemAdv Header="Lap Time Simulation - Batch"/>
                    <syncfusion:TreeViewItemAdv Header="Lap Time Simulation - Setup Optimization"/>
                    <syncfusion:TreeViewItemAdv Header="Path Optimization"/>
                    <syncfusion:TreeViewItemAdv Header="Lap Time Simulation - Tire Model Generation"/>
                </syncfusion:TreeViewItemAdv>
                <!-- Results folder -->
                <syncfusion:TreeViewItemAdv Header="Results">
                    <!-- Results types subfolders -->
                    <syncfusion:TreeViewItemAdv Header="GGV Diagram"/>
                    <syncfusion:TreeViewItemAdv Header="Lap Time Simulation - Single"/>
                    <syncfusion:TreeViewItemAdv Header="Lap Time Simulation - Batch"/>
                    <syncfusion:TreeViewItemAdv Header="Lap Time Simulation - Setup Optimization"/>
                    <syncfusion:TreeViewItemAdv Header="Path Optimization"/>
                    <syncfusion:TreeViewItemAdv Header="Lap Time Simulation - Tire Model Generation"/>
                </syncfusion:TreeViewItemAdv>
            </syncfusion:TreeViewAdv>
            <!-- Vehicle Definition Environment Tabs -->
            <TabControl Name="vehicleInputEnvironment" Grid.Row="0" Grid.Column="1" Visibility="Hidden">
                <!-- One wheel model inputs grid -->
                <TabItem Header="One Wheel">
                    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="360"/>
                                <RowDefinition Height="360"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="200"/>
                                <ColumnDefinition Width="200"/>
                                <ColumnDefinition Width="400"/>
                                <ColumnDefinition Width="585"/>
                            </Grid.ColumnDefinitions>
                            <!-- Edit Cars GroupBox -->
                            <GroupBox Header="Car and Setup" Grid.Row="0" Grid.Column="0" Margin="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <!-- Car and Setup Listbox -->
                                    <ListBox Name="oneWheelCarAndSetupListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="2" SelectionChanged="OneWheelCarAndSetupListBox_SelectionChanged"/>
                                    <!-- Car and Setup Listbox add and delete buttons -->
                                    <Button Name="oneWheelAddCarAndSetupToListBox" Content="Add" Click="OneWheelAddCarAndSetupToListBox_Click" Grid.Row="1" Grid.Column="0" Margin="2"/>
                                    <Button Name="oneWheelDeleteCarAndSetupOfListBox" Content="Delete" Click="OneWheelDeleteCarAndSetupOfListBox_Click" Grid.Row="1" Grid.Column="1" Margin="2"/>
                                    <!-- Car ID line -->
                                    <TextBlock Text="Car ID" Grid.Row="2" VerticalAlignment="Center"/>
                                    <TextBox Name="oneWheelCarIDTextBox" Grid.Row="2" Grid.Column="1" Margin="2"/>
                                    <!-- Setup ID line -->
                                    <TextBlock Text="Setup ID" Grid.Row="3" VerticalAlignment="Center"/>
                                    <TextBox Name="oneWheelSetupIDTextBox" Grid.Row="3" Grid.Column="1" Margin="2"/>
                                    <!-- Inertia selection line -->
                                    <TextBlock Text="Inertia" Grid.Row="4" VerticalAlignment="Center"/>
                                    <ComboBox Name="oneWheelInertiaCombobox" Grid.Row="4" Grid.Column="1" Margin="2" 
                                          ItemsSource="{Binding ElementName=oneWheelInertiaListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- Tire selection line -->
                                    <TextBlock Text="Tire" Grid.Row="5" VerticalAlignment="Center"/>
                                    <ComboBox Name="oneWheelTireCombobox" Grid.Row="5" Grid.Column="1" Margin="2"
                                          ItemsSource="{Binding ElementName=oneWheelTireListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- Engine selection line -->
                                    <TextBlock Text="Engine" Grid.Row="6" VerticalAlignment="Center"/>
                                    <ComboBox Name="oneWheelEngineCombobox" Grid.Row="6" Grid.Column="1" Margin="2"
                                          ItemsSource="{Binding ElementName=oneWheelEngineListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- Transmission selection line -->
                                    <TextBlock Text="Transmission" Grid.Row="7" VerticalAlignment="Center"/>
                                    <ComboBox Name="oneWheelTransmissionCombobox" Grid.Row="7" Grid.Column="1" Margin="2"
                                          ItemsSource="{Binding ElementName=oneWheelTransmissionListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- Aerodynamics selection line -->
                                    <TextBlock Text="Aerodynamics" Grid.Row="8" VerticalAlignment="Center"/>
                                    <ComboBox Name="oneWheelAerodynamicsCombobox" Grid.Row="8" Grid.Column="1" Margin="2"
                                          ItemsSource="{Binding ElementName=oneWheelAerodynamicsListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- DRS selection line -->
                                    <TextBlock Text="DRS" Grid.Row="9" VerticalAlignment="Center"/>
                                    <ComboBox Name="oneWheelDRSCombobox" Grid.Row="9" Grid.Column="1" Margin="2"
                                          ItemsSource="{Binding ElementName=oneWheelAerodynamicsListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- Suspension selection line -->
                                    <TextBlock Text="Suspension" Grid.Row="10" VerticalAlignment="Center"/>
                                    <ComboBox Name="oneWheelSuspensionCombobox" Grid.Row="10" Grid.Column="1" Margin="2"
                                          ItemsSource="{Binding ElementName=oneWheelSuspensionListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- Brakes selection line -->
                                    <TextBlock Text="Brakes" Grid.Row="11" VerticalAlignment="Center"/>
                                    <ComboBox Name="oneWheelBrakesCombobox" Grid.Row="11" Grid.Column="1" Margin="2"
                                          ItemsSource="{Binding ElementName=oneWheelBrakesListBox, Path=Items, Mode=OneWay}"/>
                                </Grid>
                            </GroupBox>
                            <!-- Edit Inertia GroupBox -->
                            <GroupBox Header="Inertia" Grid.Row="1" Grid.Column="0" Margin="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="85"/>
                                        <ColumnDefinition Width="50"/>
                                        <ColumnDefinition Width="35"/>
                                    </Grid.ColumnDefinitions>
                                    <!-- Inertia listbox -->
                                    <ListBox Name="oneWheelInertiaListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Margin="2" SelectionChanged="OneWheelInertiaListBox_SelectionChanged"/>
                                    <!-- Inertia listbox add and delete buttons-->
                                    <Button Name="oneWheelAddInertiaToListBox" Content="Add" Click="OneWheelAddInertiaToListBox_Click" Grid.Row="1" Grid.Column="0" Margin="2"/>
                                    <Button Name="oneWheelDeleteInertiaOfListBox" Content="Delete" Click="OneWheelDeleteInertiaOfListBox_Click" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Margin="2"/>
                                    <!-- Inertia ID line -->
                                    <TextBlock Text="Inertia ID" Grid.Row="2" VerticalAlignment="Center"/>
                                    <TextBox Name="oneWheelInertiaIDTextBox" Grid.Row="2" Grid.Column="1"  Grid.ColumnSpan="2" Margin="2"/>
                                    <!-- Total mass line -->
                                    <TextBlock Text="Total Mass" Grid.Row="3" VerticalAlignment="Center"/>
                                    <syncfusion:DoubleTextBox Name="oneWheelTotalMassTextBox" Grid.Row="3" Grid.Column="1" Margin="2" NumberDecimalDigits="3"/>
                                    <TextBlock Text="kg" Grid.Row="3" Grid.Column="2" VerticalAlignment="Center"/>
                                    <!-- Unsprung mass line -->
                                    <TextBlock Text="Uns. Mass" Grid.Row="4" VerticalAlignment="Center"/>
                                    <syncfusion:DoubleTextBox Name="oneWheelUnsprungMassTextbox" Grid.Row="4" Grid.Column="1" Margin="2" NumberDecimalDigits="3"/>
                                    <TextBlock Text="kg" Grid.Row="4" Grid.Column="2" VerticalAlignment="Center"/>
                                    <!-- Rotational parts moment of inertia line -->
                                    <TextBlock Text="Rot. Parts M.I." Grid.Row="5" VerticalAlignment="Center"/>
                                    <syncfusion:DoubleTextBox Name="oneWheelRotPartsMITextbox" Grid.Row="5" Grid.Column="1" Margin="2" NumberDecimalDigits="3"/>
                                    <TextBlock Text="kg.m²" Grid.Row="5" Grid.Column="2" VerticalAlignment="Center"/>
                                    <!-- Gravity acceleration line -->
                                    <TextBlock Text="Gravity Accel." Grid.Row="6" VerticalAlignment="Center"/>
                                    <syncfusion:DoubleTextBox Name="oneWheelGravityAccelTextbox" Grid.Row="6" Grid.Column="1" Margin="2" NumberDecimalDigits="3"/>
                                    <TextBlock Text="m/s²" Grid.Row="6" Grid.Column="2" VerticalAlignment="Center"/>
                                </Grid>
                            </GroupBox>
                            <!-- Edit Supension GroupBox -->
                            <GroupBox Header="Suspension" Grid.Row="0" Grid.Column="1" Margin="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="85"/>
                                        <ColumnDefinition Width="50"/>
                                        <ColumnDefinition Width="35"/>
                                    </Grid.ColumnDefinitions>
                                    <!-- Suspension Listbox -->
                                    <ListBox Name="oneWheelSuspensionListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Margin="2" SelectionChanged="OneWheelSuspensionListBox_SelectionChanged"/>
                                    <!-- Suspension listbox add and delete buttons -->
                                    <Button Name="oneWheelAddSuspensionToListBox" Content="Add" Click="OneWheelAddSuspensionToListBox_Click" Grid.Row="1" Grid.Column="0" Margin="2"/>
                                    <Button Name="oneWheelDeleteSuspensionOfListBox" Content="Delete" Click="OneWheelDeleteSuspensionOfListBox_Click" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Margin="2"/>
                                    <!-- Suspension ID line -->
                                    <TextBlock Text="Suspension ID" Grid.Row="2" VerticalAlignment="Center"/>
                                    <TextBox Name="oneWheelSuspensionIDTextBox" Grid.Row="2" Grid.Column="1"  Grid.ColumnSpan="2" Margin="2"/>
                                    <!-- Heave stiffness line -->
                                    <TextBlock Text="Heave Stiffness" Grid.Row="3" VerticalAlignment="Center"/>
                                    <syncfusion:DoubleTextBox Name="oneWheelHeaveStiffnessTextBox" Grid.Row="3" Grid.Column="1" Margin="2" NumberDecimalDigits="3"/>
                                    <TextBlock Text="N/mm" Grid.Row="3" Grid.Column="2" VerticalAlignment="Center"/>
                                    <!-- Car height line-->
                                    <TextBlock Text="Car Height" Grid.Row="4" VerticalAlignment="Center"/>
                                    <syncfusion:DoubleTextBox Name="oneWheelCarHeightTextbox" Grid.Row="4" Grid.Column="1" Margin="2" NumberDecimalDigits="3"/>
                                    <TextBlock Text="mm" Grid.Row="4" Grid.Column="2" VerticalAlignment="Center"/>
                                </Grid>
                            </GroupBox>
                            <!-- Edit Brakes GroupBox -->
                            <GroupBox Header="Brakes" Grid.Row="1" Grid.Column="1" Margin="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="85"/>
                                        <ColumnDefinition Width="50"/>
                                        <ColumnDefinition Width="35"/>
                                    </Grid.ColumnDefinitions>
                                    <!-- Brakes listbox -->
                                    <ListBox Name="oneWheelBrakesListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Margin="2" SelectionChanged="OneWheelBrakesListBox_SelectionChanged"/>
                                    <!-- Brakes listbox add and delete buttons -->
                                    <Button Name="oneWheelAddBrakesToListBox" Content="Add" Click="OneWheelAddBrakesToListBox_Click" Grid.Row="1" Grid.Column="0" Margin="2"/>
                                    <Button Name="oneWheelDeleteBrakesOfListBox" Content="Delete" Click="OneWheelDeleteBrakesOfListBox_Click" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Margin="2"/>
                                    <!-- Brakes ID line -->
                                    <TextBlock Text="Brakes ID" Grid.Row="2" VerticalAlignment="Center"/>
                                    <TextBox Name="oneWheelBrakesIDTextBox" Grid.Row="2" Grid.Column="1"  Grid.ColumnSpan="2" Margin="2"/>
                                    <!-- Maximum torque line -->
                                    <TextBlock Text="Max. Torque" Grid.Row="3" VerticalAlignment="Center"/>
                                    <syncfusion:DoubleTextBox Name="oneWheelBrakesMaxTorqueTextBox" Grid.Row="3" Grid.Column="1" Margin="2" NumberDecimalDigits="3"/>
                                    <TextBlock Text="N.m" Grid.Row="3" Grid.Column="2" VerticalAlignment="Center"/>
                                </Grid>
                            </GroupBox>
                            <!-- Edit Tire GroupBox -->
                            <GroupBox Header="Tire" Grid.Row="0" Grid.Column="2" Margin="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                        <RowDefinition Height="25"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="85"/>
                                        <ColumnDefinition Width="50"/>
                                        <ColumnDefinition Width="35"/>
                                        <ColumnDefinition Width="85"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <!-- Tire listbox-->
                                    <ListBox Name="oneWheelTireListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Margin="2" SelectionChanged="OneWheelTireListBox_SelectionChanged"/>
                                    <!-- Tire listbox add and delete buttons -->
                                    <Button Name="oneWheelAddTireToListBox" Content="Add" Click="OneWheelAddTireToListBox_Click" Grid.Row="1" Grid.Column="0" Margin="2"/>
                                    <Button Name="oneWheelDeleteTireOfListBox" Content="Delete" Click="OneWheelDeleteTireOfListBox_Click" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Margin="2"/>
                                    <!-- Tire ID line -->
                                    <TextBlock Text="Tire ID" Grid.Row="2" VerticalAlignment="Center"/>
                                    <TextBox Name="oneWheelTireIDTextBox" Grid.Row="2" Grid.Column="1"  Grid.ColumnSpan="2" Margin="2"/>
                                    <!-- Tire vertical stiffness line -->
                                    <TextBlock Text="Vert. Stiffness" Grid.Row="3" VerticalAlignment="Center"/>
                                    <syncfusion:DoubleTextBox Name="oneWheelTireStiffnessTextBox" Grid.Row="3" Grid.Column="1" Margin="2" NumberDecimalDigits="3"/>
                                    <TextBlock Text="N/mm" Grid.Row="3" Grid.Column="2" VerticalAlignment="Center"/>
                                    <!-- Tire model file selection line -->
                                    <TextBlock Text="Tire Model" Grid.Row="4" VerticalAlignment="Center"/>
                                    <TextBox Name="oneWheelTireModelTextBox" Grid.Row="4" Grid.Column="1"  Grid.ColumnSpan="2" Margin="2"/>
                                    <Button Name="oneWheelTireModelButton" Content="Browse" Click="OneWheelTireModelButton_Click" Grid.Row="4" Grid.Column="3" Margin="2"/>
                                    <!-- Tire model scalling factors groupbox -->
                                    <GroupBox Header="Scalling Factors" Grid.Row="0" Grid.Column="3" Grid.RowSpan="4" Grid.ColumnSpan="2" Margin="2">
                                        <Grid Margin="2">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="2*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="2*"/>
                                            </Grid.ColumnDefinitions>
                                            <!-- lambdaFzO -->
                                            <TextBlock HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" 
										   FontSize="15" Margin="0">
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="Fzo"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                            <syncfusion:DoubleTextBox Name="oneWheelLambdaFzOTextBox" Text="1" Grid.Row="0" Grid.Column="1"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Left" VerticalContentAlignment="Center" 
														  Margin="1"></syncfusion:DoubleTextBox>
                                            <!-- lambdaMuV -->
                                            <TextBlock Grid.Row="0" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center" 
										   FontSize="15" Margin="0">
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="μV"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                            <syncfusion:DoubleTextBox Name="oneWheelLambdaMuVTextBox" Text="1" Grid.Row="0" Grid.Column="3"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Left" VerticalContentAlignment="Center" 
														  Margin="1"></syncfusion:DoubleTextBox>
                                            <!-- lambdaMux -->
                                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" 
										   FontSize="15" Margin="0">
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="μx"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                            <syncfusion:DoubleTextBox Name="oneWheelLambdaMuxTextBox" Text="1" Grid.Row="1" Grid.Column="1"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Left" VerticalContentAlignment="Center" 
														  Margin="1"></syncfusion:DoubleTextBox>
                                            <!-- lambdaMuy -->
                                            <TextBlock Grid.Row="1" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center" 
										   FontSize="15" Margin="0">
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="μy"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                            <syncfusion:DoubleTextBox Name="oneWheelLambdaMuyTextBox" Text="1" Grid.Row="1" Grid.Column="3"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Left" VerticalContentAlignment="Center" 
														  Margin="1"></syncfusion:DoubleTextBox>
                                            <!-- lambdaKxk -->
                                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" 
										   FontSize="15" Margin="0">
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="xκ"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                            <syncfusion:DoubleTextBox Name="oneWheelLambdaKxkTextBox" Text="1" Grid.Row="2" Grid.Column="1"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Left" VerticalContentAlignment="Center" 
														  Margin="1"></syncfusion:DoubleTextBox>
                                            <!-- lambdaKya -->
                                            <TextBlock Grid.Row="2" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center" 
										   FontSize="15" Margin="0">
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="yα"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                            <syncfusion:DoubleTextBox Name="oneWheellambdaKyaTextBox" Text="1" Grid.Row="2" Grid.Column="3"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Left" VerticalContentAlignment="Center" 
														  Margin="1"></syncfusion:DoubleTextBox>
                                            <!-- lambdaCx -->
                                            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" 
										   FontSize="15" Margin="0">
									<Run>λ</Run>
									<InlineUIContainer>
										<TextBlock FontSize="9" Text="Cx"></TextBlock>
									</InlineUIContainer>
								</TextBlock>
                                            <syncfusion:DoubleTextBox Name="oneWheelLambdaCxTextBox" Text="1" Grid.Row="3" Grid.Column="1"
														  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
														  HorizontalContentAlignment="Left" VerticalContentAlignment="Center" 
														  Margin="1"></syncfusion:DoubleTextBox>
                                            <!-- lambdaCy -->
                                            <TextBlock Grid.Row="3" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center" 
										   FontSize="15" Margin="0">
									<Run>λ</Run>
									<InlineUIContainer>
