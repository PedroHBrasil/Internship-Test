                                            <syncfusion:SfDataGrid Name="tabularPathSectorsSfDataGrid" ItemsSource="{Binding PathSectors, Mode=TwoWay}"
                                                               AddNewRowPosition="Bottom" HeaderRowHeight="30" Margin="2" LiveDataUpdateMode="AllowDataShaping"
                                                               AllowEditing="True" AllowDeleting="True" AllowSorting="True">
                                                <syncfusion:SfDataGrid.Columns>
                                                    <syncfusion:GridNumericColumn HeaderText="ID" MappingName="SectorID" Width="30" TextWrapping="Wrap" NumberDecimalDigits="0"/>
                                                    <syncfusion:GridNumericColumn HeaderText="Start Distance (m)" MappingName="SectorStartDistance" Width="130" NumberDecimalDigits="3"/>
                                                    <syncfusion:GridCheckBoxColumn HeaderText="DRS" MappingName="IsDRSEnabled" Width="50"/>
                                                </syncfusion:SfDataGrid.Columns>
                                            </syncfusion:SfDataGrid>
                                        </Grid>
                                    </GroupBox>
                                    <!-- Path sections input groupbox -->
                                    <GroupBox Header="Sections" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="4">
                                        <Grid>
                                            <Grid.Resources>
                                                <ComboBox x:Key="tabularPathComboboxItems">
                                                    <ComboBoxItem Content="Straight"/>
                                                    <ComboBoxItem Content="Left"/>
                                                    <ComboBoxItem Content="Right"/>
                                                </ComboBox>
                                            </Grid.Resources>
                                            <Grid.DataContext>
                                                <local:TabularPathSectionViewModel/>
                                            </Grid.DataContext>
                                            <ComboBox Name="tabularPathSectionTypeItemsSourceCombobox" Visibility="Hidden">
                                                <ComboBoxItem Content="Straight"/>
                                                <ComboBoxItem Content="Left"/>
                                                <ComboBoxItem Content="Right"/>
                                            </ComboBox>
                                            <syncfusion:SfDataGrid Name="tabularPathSectionsSfDataGrid" AutoGenerateColumns="True" ItemsSource="{Binding TabularPathSections, Mode=TwoWay}"
                                                               AddNewRowPosition="Bottom" ColumnSizer="SizeToHeader" HeaderRowHeight="30" Margin="2" LiveDataUpdateMode="AllowDataShaping"
                                                               AllowEditing="True" AllowDeleting="True" AllowSorting="True">
                                                <syncfusion:SfDataGrid.Columns>
                                                    <syncfusion:GridNumericColumn HeaderText="Length (m)" MappingName="Length" Width="80" NumberDecimalDigits="3"/>
                                                    <syncfusion:GridNumericColumn HeaderText="Initial Radius (m)" MappingName="InitialRadius" Width="115" NumberDecimalDigits="3"/>
                                                    <syncfusion:GridNumericColumn HeaderText="Final Radius (m)" MappingName="FinalRadius" Width="115" NumberDecimalDigits="3"/>
                                                </syncfusion:SfDataGrid.Columns>
                                            </syncfusion:SfDataGrid>
                                        </Grid>
                                    </GroupBox>
                                </Grid>
                            </GroupBox>
                            <!-- Path Visualization Groupbox -->
                            <GroupBox Header="Path Visualization" Grid.Column="1">
                                <Grid Name="tabularPathVisualizationGrid" Margin="5"/>
                            </GroupBox>
                        </Grid>
                    </ScrollViewer>
                </TabItem>
                <TabItem Header="Draw">

                </TabItem>
                <TabItem Header="Optimization">

                </TabItem>
            </TabControl>
            <!-- Simulation Definition Environment Tabs -->
            <TabControl Name="simulationInputEnvironment" Grid.Row="0" Grid.Column="1" Visibility="Visible">
                <TabItem Header="GGV Diagram">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="250"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <!-- GGV Generation Inputs Groupbox -->
                        <GroupBox Header="GGV Inputs">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="200"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="2*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <!-- GGV Diagrams Listbox -->
                                <ListBox Name="GGVDiagramListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="6" Margin="2" SelectionChanged="GGVDiagramListBox_SelectionChanged"/>
                                <!-- GGV Diagrams Listbox buttons -->
                                <Button Content="Add" Name="addGGVDiagramToListboxButton" Click="AddGGVDiagramToListboxButton_Click" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Margin="2"/>
                                <Button Content="Delete" Name="deleteGGVDiagramOfListboxButton" Click="DeleteGGVDiagramOfListboxButton_Click" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="3" Margin="2"/>
                                <!-- Car and Setup Input Line -->
                                <TextBlock Text="Car/Setup" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="2"/>
                                <ComboBox Name="GGVDiagramCarAndSetupCombobox" Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="4" Margin="2" 
                                          ItemsSource="{Binding ElementName=oneWheelCarAndSetupListBox, Path=Items, Mode=OneWay}"/>
                                <!-- Amount of Points per GG Diagram Line -->
                                <TextBlock Text="Amount of Points per Speed" Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4" VerticalAlignment="Center" Margin="2"/>
                                <syncfusion:DoubleTextBox Name="GGVDiagramAmountOfPointsPerSpeedTextBox" Text="0" Grid.Row="3" Grid.Column="4" 
                                                               Grid.ColumnSpan="2" HorizontalContentAlignment="Right" Margin="2" NumberDecimalDigits="0"/>
                                <!-- Amount of Speeds Line -->
                                <TextBlock Text="Amount of Speeds" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" VerticalAlignment="Center" Margin="2"/>
                                <syncfusion:DoubleTextBox Name="GGVDiagramAmountOfSpeedsTextBox" Text="0" Grid.Row="4" Grid.Column="4" 
                                                               Grid.ColumnSpan="2" HorizontalContentAlignment="Right" Margin="2" NumberDecimalDigits="0"/>
                                <!-- Lowest Speed Line -->
                                <TextBlock Text="Lowest Speed" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="2"/>
                                <syncfusion:DoubleTextBox Name="GGVDiagramLowestSpeedTextBox" Text="0" Grid.Row="5" Grid.Column="2" 
                                                               Grid.ColumnSpan="3" HorizontalContentAlignment="Right" Margin="2" NumberDecimalDigits="2"/>
                                <TextBlock Text="km/h" Grid.Row="5" Grid.Column="5" VerticalAlignment="Center" Margin="2"/>
                                <!-- Highest Speed Line -->
                                <TextBlock Text="Highest Speed" Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="2"/>
                                <syncfusion:DoubleTextBox Name="GGVDiagramHighestSpeedTextBox" Text="0" Grid.Row="6" Grid.Column="2" 
                                                               Grid.ColumnSpan="3" HorizontalContentAlignment="Right" Margin="2" NumberDecimalDigits="2"/>
                                <TextBlock Text="km/h" Grid.Row="6" Grid.Column="5" VerticalAlignment="Center" Margin="2"/>
                            </Grid>
                        </GroupBox>
                        <GroupBox Header="GGV Visualization" Grid.Row="0" Grid.Column="1">
                            <Grid Name="GGVDiagramVisualizationGrid" Margin="5">
                                <!--
                                <syncfusion:SfSurfaceChart Name="GGVDiagramVisualizationSurfaceChart" 
                                                           Header="GGV Diagram" FontSize="20" 
                                                           EnableRotation="True" EnableZooming="True">
                                </syncfusion:SfSurfaceChart>
                                    -->
                            </Grid>
                        </GroupBox>
                    </Grid>
                </TabItem>

                <TabItem Header="Lap Time Simulation">
                    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="250"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <!-- GGV Generation Inputs Groupbox -->
                            <GroupBox Header="Lap Time Simulation Inputs">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="200"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="2*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <!-- Lap Time Simulation Listbox -->
                                    <ListBox Name="lapTimeSimulationListBox" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="6" Margin="2" SelectionChanged="LapTimeSimulationListBox_SelectionChanged"/>
                                    <!-- Lap Time Simulation Listbox buttons -->
                                    <Button Content="Add" Name="addLapTimeSimulationToListboxButton" Click="AddLapTimeSimulationToListboxButton_Click" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Margin="2"/>
                                    <Button Content="Delete" Name="deleteLapTimeSimulationOfListboxButton" Click="DeleteLapTimeSimulationOfListboxButton_Click" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="3" Margin="2"/>
                                    <!-- GGV Diagram Input Line -->
                                    <TextBlock Text="GGV Diagram" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="2"/>
                                    <ComboBox Name="LapTimeSimulationGGVDiagramCombobox" Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="4" Margin="2" 
                                          ItemsSource="{Binding ElementName=GGVDiagramListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- Path Input Line -->
                                    <TextBlock Text="Path" Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="2"/>
                                    <ComboBox Name="LapTimeSimulationPathCombobox" Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="4" Margin="2" 
                                          ItemsSource="{Binding ElementName=tabularPathListBox, Path=Items, Mode=OneWay}"/>
                                    <!-- Mode Input Line -->
                                    <TextBlock Text="Mode" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="2"/>
                                    <ComboBox Name="LapTimeSimulationModeCombobox" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="4" Margin="2">
                                        <ComboBoxItem Content="First Lap"/>
                                        <ComboBoxItem Content="Normal Lap"/>
                                    </ComboBox>
                                </Grid>
                            </GroupBox>
                        </Grid>
                    </ScrollViewer>
                </TabItem>
            </TabControl>
            <!-- Results Analysis Environment -->
            <TabControl Name="resultsAnalysisEnvironment" Grid.Column="1" Visibility="Hidden">
                <TabItem Header="Lap Time Simulation">
                    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                        <Grid Name="resultsAnalysisLapTimeSimulationEnvironment" Grid.Row="0" Grid.Column="1" Visibility="Visible">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="80"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="250"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <!-- Data Control Groupbox -->
                                <GroupBox Header="Data Control" Grid.Row="0" Grid.Column="0">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <!-- Simulation type combobox -->
                                        <TextBlock Text="Simulations Results Selection" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4" VerticalAlignment="Center" Margin="2"/>
                                        <syncfusion:CheckListBox Name="resultsAnalysisLapTimeSimulationChecklistbox" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="4" Margin="2"
                                                                 ItemsSource="{Binding ElementName=lapTimeSimulationListBox, Path=Items, Mode=OneWay}" SelectionChanged="ResultsAnalysisLapTimeSimulationXAxisDataCombobox_DropDownClosed"/>
                                    </Grid>
                                </GroupBox>
                                <!-- Charts groupbox -->
                                <GroupBox Header="Chart" Grid.Row="0" Grid.Column="1" Grid.RowSpan="2">
                                    <Grid Name="resultsAnalysisLapTimeSimulationChartGrid" Margin="10"/>
                                </GroupBox>
                                <!-- Current chart data controls groupbox -->
                                <GroupBox Header="Chart Axis" Grid.Row="1" Grid.Column="0">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <!-- X axis line -->
                                        <TextBlock Text="X Data" Grid.Row="0" Grid.ColumnSpan="2" VerticalAlignment="Center"/>
                                        <ComboBox Name="resultsAnalysisLapTimeSimulationXAxisDataCombobox" Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="3" Margin="2" 
                                                          DropDownClosed="ResultsAnalysisLapTimeSimulationXAxisDataCombobox_DropDownClosed">
                                            <ComboBoxItem Content="Time"/>
                                            <ComboBoxItem Content="Distance"/>
                                            <ComboBoxItem Content="Speed"/>
                                            <ComboBoxItem Content="Longitudinal Acceleration"/>
                                            <ComboBoxItem Content="Lateral Acceleration"/>
                                            <ComboBoxItem Content="Gear"/>
                                        </ComboBox>
                                        <!-- Y axis line -->
                                        <TextBlock Text="Y Data" Grid.Row="1" Grid.ColumnSpan="2" VerticalAlignment="Center"/>
                                        <ComboBox Name="resultsAnalysisLapTimeSimulationYAxisDataCombobox" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Margin="2" 
                                                          DropDownClosed="ResultsAnalysisLapTimeSimulationXAxisDataCombobox_DropDownClosed">
                                            <ComboBoxItem Content="Time"/>
                                            <ComboBoxItem Content="Distance"/>
                                            <ComboBoxItem Content="Speed"/>
                                            <ComboBoxItem Content="Longitudinal Acceleration"/>
                                            <ComboBoxItem Content="Lateral Acceleration"/>
                                            <ComboBoxItem Content="Gear"/>
                                        </ComboBox>
                                    </Grid>
                                </GroupBox>
                            </Grid>
                        </Grid>
                    </ScrollViewer>
                </TabItem>
            </TabControl>


        </Grid>
    </Grid>
</syncfusion:RibbonWindow>
